<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="aymerich.ioc.cat.tea2_clientm_aymerichs.activities.Registre">


        <TextView
            android:id="@+id/tv_rol_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_provincia_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="-2dp"
            android:layout_marginLeft="-2dp"
            android:layout_marginTop="12dp"
            android:fontFamily="monospace"
            android:text="@string/tv_rol_registre"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/et_direccio_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_direccio_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:layout_marginStart="-2dp"
            android:layout_marginLeft="-2dp"
            android:layout_marginTop="0dp"
            android:autofillHints=""
            android:ems="17" />

        <EditText
            android:id="@+id/et_poblacio_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_poblacio_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:layout_marginStart="-3dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginTop="2dp"
            android:autofillHints=""
            android:ems="17" />

        <EditText
            android:id="@+id/et_provincia_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_provincia_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:layout_marginStart="-3dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginTop="3dp"
            android:autofillHints=""
            android:ems="17" />

        <EditText
            android:id="@+id/et_rol_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_rol_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:layout_marginStart="-2dp"
            android:layout_marginLeft="-2dp"
            android:layout_marginTop="1dp"
            android:autofillHints=""
            android:ems="17" />

        <EditText
            android:id="@+id/et_cif_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_cif_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:layout_marginStart="1dp"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="2dp"
            android:autofillHints=""
            android:ems="17" />

        <TextView
            android:id="@+id/tv_nom_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_password_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="10dp"
            android:fontFamily="monospace"
            android:text="@string/tv_nom_registre"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/tv_cif_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_nom_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="11dp"
            android:fontFamily="monospace"
            android:text="@string/tv_cif_empresa"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/tv_direccio_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_cif_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="13dp"
            android:fontFamily="monospace"
            android:text="@string/tv_direccio_registre"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/tv_poblacio_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_direccio_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="1dp"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="10dp"
            android:fontFamily="monospace"
            android:text="@string/tv_poblacio_registre"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/tv_provincia_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_poblacio_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="-2dp"
            android:layout_marginLeft="-2dp"
            android:layout_marginTop="10dp"
            android:fontFamily="monospace"
            android:text="@string/tv_provincia_registre"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/tv_registre_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="47dp"
            android:fontFamily="sans-serif-condensed"
            android:text="@string/registre"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tv_email_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_registre_registre"
            android:layout_alignStart="@+id/tv_registre_registre"
            android:layout_alignLeft="@+id/tv_registre_registre"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="28dp"
            android:fontFamily="monospace"
            android:text="@string/tv_email_registre"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/et_email_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_email_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:autofillHints=""
            android:ems="17" />


        <EditText
            android:id="@+id/et_nom_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_nom_registre"
            android:layout_alignStart="@+id/tv_email_registre"
            android:layout_alignLeft="@+id/tv_email_registre"
            android:layout_marginStart="-1dp"
            android:layout_marginLeft="-1dp"
            android:layout_marginTop="1dp"
            android:autofillHints=""
            android:ems="17" />

        <TextView
            android:id="@+id/tv_password_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_email_registre"
            android:layout_alignStart="@+id/et_email_registre"
            android:layout_alignLeft="@+id/et_email_registre"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="14dp"
            android:fontFamily="monospace"
            android:text="@string/contrasenya"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/et_password_registre"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_password_registre"
            android:layout_alignStart="@+id/tv_password_registre"
            android:layout_alignLeft="@+id/tv_password_registre"
            android:autofillHints=""
            android:ems="17"
            android:inputType="textPassword"
            android:textSize="12sp" />


        <Button
            android:id="@+id/b_enviar_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/et_rol_registre"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="103dp"
            android:layout_marginLeft="103dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="106dp"
            android:layout_marginRight="106dp"
            android:ems="12"
            android:text="@string/bt_registrar_registre" />

        <Button
            android:id="@+id/b_tornar_registre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/b_enviar_registre"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="103dp"
            android:layout_marginLeft="103dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="106dp"
            android:layout_marginRight="106dp"
            android:ems="12"
            android:text="@string/b_tornar_registre" />

        <TextView
            android:id="@+id/tv_titol_registre"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="116dp"
            android:layout_marginLeft="116dp"
            android:layout_marginTop="16dp"
            android:text="@string/fem_coworking"
            android:textSize="25sp" />

    </RelativeLayout>
</ScrollView>